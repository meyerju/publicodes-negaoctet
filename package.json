{
  "name": "@incubateur-ademe/modele-numerique",
  "version": "0.1.8",
  "description": "Modèle Publicodes pour calculer l'impact (en CO2eq) du numérique",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "model.json",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rm -r model.json index.d.ts index.js",
    "build": "node build.js",
    "doc:start": "yarn run build && cp model.json doc/src && cd doc && yarn run start",
    "doc:deploy": "yarn run build && cp model.json doc/src && cd doc && yarn run deploy"
  },
  "repository": "git@github.com:incubateur-ademe/modele-numerique.git",
  "homepage": "https://github.com/incubateur-ademe/modele-numerique#readme",
  "bugs": {
    "url": "https://github.com/incubateur-ademe/modele-numerique/issues"
  },
  "authors": [
    "Clément Auger <clement.auger@beta.gouv.fr>",
    "Emile Rolley <emile.rolley@tuta.io>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@incubateur-ademe/publicodes-tools": "^0.2.1"
  },
  "dependencies": {
    "publicodes": "^1.0.0-beta.71"
  },
  "publishConfig": {
    "access": "public"
  }
}
